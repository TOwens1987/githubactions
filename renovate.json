{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],

  "terragrunt": {
    "fileMatch": ["^terragrunt/.*\\.hcl$"]
  },

  "regexManagers": [
  {
    "fileMatch": ["^terragrunt/.*\\.hcl$"],
    "matchStringsStrategy": "any",
    "matchStrings": [
      "Name = appdynamics",
      "enabled = true",
      "revision = (?<currentValue>\\S*)"
      ],
      "datasourceTemplate": "terragrunt"
    }
  ]
}